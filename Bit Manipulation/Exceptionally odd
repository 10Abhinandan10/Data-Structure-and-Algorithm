package bit_Manupulation;
import java.util.Scanner;
public class P_40 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int arr[] = {1, 2, 3, 2, 3, 1, 3};
		int n=arr.length;
		int x=0;
        for(int i=0;i<n;i=i+1){
            x=x^arr[i];
        }
        System.out.println(x);
	}
}
