package bit_Manupulation;
import java.util.Scanner;
public class P_38 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int arr[]={10, 11, 1, 2, 3};
		int n=arr.length;
		int a[]=new int[n];
        for(int i=0;i<n-1;i=i+1){
            a[i]=arr[i]|arr[i+1];
        }
        a[n-1]=arr[n-1];
        for(int i=0;i<n;i=i+1) {
        	System.out.print(a[i]+" ");
        }
	}
}
