package bit_Manupulation;
import java.util.Scanner;
public class P_14 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n1=sc.nextInt();
		sc.nextLine();
		String n=sc.nextLine();
		int l=n.length();
		String s="";
		for(int i=0;i<l;i=i+1) {
			if(n.charAt(i)=='0') {
				s=s+'1';
			}else {
				s=s+'0';
			}
		}
		System.out.println(s);
	}
}
