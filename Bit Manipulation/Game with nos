package bit_Manupulation;
import java.util.Scanner;
public class P_28 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a[]={10, 11, 1, 2, 3};
		int n=a.length;
		int aa[]=new int [n];
		for(int i=0;i<n-1;i=i+1) {
			aa[i]=a[i]^a[i+1];
		}
		aa[n-1]=a[n-1];
		System.out.println(aa);
	}
}
