package bit_Manupulation;
import java.util.Scanner;
public class P_21 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Emter N: ");
		int n=sc.nextInt();
		int count=0;
		for(int i=0;i<n;i=i+1) {
			if((n^i)==n+i) {
				count =count+1;
			}
		}
		System.out.println(count);
	}
}
